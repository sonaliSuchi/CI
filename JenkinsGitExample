pipeline {
  agent any
  parameters {
    gitParameter branchFilter: 'origin/(.*)', defaultValue: 'master', name: 'BRANCH_SELECTOR', type: 'PT_BRANCH', useRepository: '.*CI.git'
  }
  stages {
    stage('Example') {
      steps {
        git branch: "${params.BRANCH_SELECTOR}", url: 'https://github.com/sonaliSuchi/CI.git'
      }
    }
  }
}